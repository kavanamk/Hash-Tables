import random

class RandomizedSet(object):

    def __init__(self):
        self.random_set = []
        self.index_map = {}
        

    def insert(self, val):
        """
        :type val: int
        :rtype: bool
        """
        if val in self.index_map:
            return False
        self.random_set.append(val)
        self.index_map[val] = len(self.random_set) - 1
        return True
        

    def remove(self, val):
        """
        :type val: int
        :rtype: bool
        """
        if val not in self.index_map:
            return False
        last_element = self.random_set[-1]
        idx_to_remove = self.index_map[val]
        # Move the last element to the place of the element to remove
        self.random_set[idx_to_remove] = last_element
        self.index_map[last_element] = idx_to_remove
        # Remove the last element
        self.random_set.pop()
        del self.index_map[val]
        return True
        

    def getRandom(self):
        """
        :rtype: int
        """
        return random.choice(self.random_set)


# Your RandomizedSet object will be instantiated and called as such:
# obj = RandomizedSet()
# param_1 = obj.insert(val)
# param_2 = obj.remove(val)
# param_3 = obj.getRandom()
